<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tool="http://schemas.android.com/tools">
	
	<data>
		<import type="android.view.View"/>
		<variable
			name="user"
			type="pick.com.app.varient.owner.pojo.BookingModel.Data"/>
	
	</data>

		
		<LinearLayout
			android:background="@color/ef_white"
			android:orientation="vertical"
			android:paddingBottom="10dp"
			android:layout_width="match_parent"
			android:layout_height="wrap_content">
			
			<View
				android:layout_width="match_parent"
				android:background="@color/gray_btn_bg_color"
				android:layout_height="1dp" />
			
			<LinearLayout
				android:layout_width="match_parent"
				android:orientation="horizontal"
				android:layout_height="wrap_content">
				
				<RelativeLayout
					android:layout_width="@dimen/_100sdp"
					
					android:layout_height="@dimen/_100sdp">
					
					<ImageView
						android:scaleType="centerCrop"
						android:layout_centerInParent="true"
						android:layout_width="wrap_content"
						app:loadImageData='@{user.upload_url +"vehicle_images/"+ user.vehicle_image}'
						android:src="@drawable/placeholder"
						android:layout_height="wrap_content" />
					
					<TextView
						android:layout_width="match_parent"
						android:background="#B6000000"
						android:layout_alignParentBottom="true"
						android:text="+919001638910"
						android:textSize="10sp"
						android:padding="3dp"
						android:visibility="gone"
						android:gravity="center"
						android:textColor="@color/ef_white"
						android:layout_height="wrap_content" />
				
				</RelativeLayout>
				
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".2"
					android:layout_gravity="center"
					android:layout_height="match_parent">
					
					<LinearLayout
						android:layout_width="match_parent"
						android:orientation="vertical"
						android:layout_height="match_parent">
						
						
						<LinearLayout
							android:layout_width="match_parent"
							android:paddingStart="5dp"
							android:gravity="center_vertical"
							android:orientation="vertical"
							android:layout_height="0dp"
							android:layout_weight=".4">
							
							<TextView
								android:layout_width="match_parent"
								tool:text="Jayden Ethan"
								android:text="@{user.vehicle_model}"
								android:textSize="13sp"
								android:textColor="@color/profile_top_back_color"
								android:layout_height="wrap_content" />
							
							<TextView
								android:layout_width="wrap_content"
								tool:text="#BGNHY2503"
								android:text='@{"#"+user.booking_id}'
								android:textSize="13sp"
								android:paddingEnd="7sp"
								android:textColor="@color/skyblue"
								android:layout_height="wrap_content" />
							
							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content">
							
							<TextView
								android:layout_width="wrap_content"
								tool:text="Free Km 250 km"
								android:visibility="@{user.vehicle_for==1?View.VISIBLE:View.INVISIBLE}"
								android:text='@{@string/free_km_with_value(user.one_day_free_kilometer)}'
								
								android:textSize="11sp"
								android:textColor="@color/profile_top_back_color"
								android:layout_height="wrap_content" />
								
								<TextView
									android:layout_width="wrap_content"
									tool:text="Excess 50 SAR/Km"
									android:layout_marginStart="7dp"
									android:visibility="@{user.vehicle_for==1?View.VISIBLE:View.GONE}"
									android:text='@{@string/excess_value(user.per_kilometer_charge)}'
									
									android:textSize="11sp"
									android:layout_height="wrap_content" />
							
							</LinearLayout>
							<LinearLayout
								android:layout_width="match_parent"
								android:orientation="horizontal"
								android:gravity="center_vertical"
								android:layout_marginTop="@dimen/_5sdp"
								android:layout_height="wrap_content">
								
								
								<androidx.appcompat.widget.AppCompatButton
									android:layout_width="wrap_content"
									android:background="#1DA1F2"
									android:layout_marginEnd="5dp"
									android:textSize="8sp"
									android:elevation="5dp"
									
									android:text="@{user.getStatusByIdUser(user.booking_status)}"
									android:textColor="@color/ef_white"
									android:padding="7dp"
									
									android:layout_height="30dp" />
								
							
							
							</LinearLayout>
						
						
						</LinearLayout>
						
						<LinearLayout
							android:layout_width="match_parent"
							android:orientation="vertical"
							android:layout_height="0dp"
							android:gravity="bottom"
							android:visibility="gone"
							android:layout_weight=".6">
						
						<LinearLayout
							android:layout_width="match_parent"
							
							android:background="#F1F1F1"
							android:padding="3dp"
							android:gravity="center_vertical"
							android:layout_height="wrap_content">
							
						
							
							
							
							<LinearLayout
								android:layout_width="0dp"
								android:layout_weight=".1"
								android:paddingStart="5dp"
								android:gravity="center_vertical"
								android:layout_height="wrap_content">
								
								
							
							
							</LinearLayout>
							
							<View
								android:layout_width="1dp"
								android:layout_height="match_parent"
								android:background="@color/gray_btn_bg_color" />
							
						
						</LinearLayout>
						<View
							android:layout_width="match_parent"
							android:layout_height="1dp"
							android:background="@color/gray_btn_bg_color" />
						<LinearLayout
							android:layout_width="match_parent"
							
							android:padding="3dp"
							android:background="#F1F1F1"
							android:gravity="center_vertical"
							android:layout_height="wrap_content">
							
							<LinearLayout
								android:layout_width="0dp"
								android:layout_weight=".1"
								android:paddingStart="5dp"
								android:layout_height="wrap_content">
								
								
							
							
							</LinearLayout>
							
							<View
								android:layout_width="1dp"
								android:layout_height="match_parent"
								android:background="@color/gray_btn_bg_color" />
							
							<LinearLayout
								android:layout_width="0dp"
								android:layout_weight=".1"
								
								android:paddingStart="15dp"
								android:layout_height="wrap_content">
								
								
							
							
							</LinearLayout>
						
						</LinearLayout>
						</LinearLayout>
					
					</LinearLayout>
				</LinearLayout>
			
			
			</LinearLayout>
			
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/gray_btn_bg_color" />
			
			<LinearLayout
				android:layout_width="match_parent"
				android:padding="5dp"
				android:paddingStart="10dp"
				android:layout_height="wrap_content">
				
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="5 Seater"
						android:text='@{user.seater + " Seater"}'
						android:textSize="12sp"
						android:textColor="#888888"
						android:layout_height="wrap_content" />
					
					<View
						android:layout_width="1dp"
						android:layout_margin="5dp"
						android:background="@color/gray_btn_bg_color"
						android:layout_height="match_parent" />
				
				
				</LinearLayout>
				
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="2016 Model"
						android:text="@{user.vehicle_year + ` Model`}"
						android:textSize="12sp"
						android:textColor="#888888"
						android:layout_height="wrap_content" />
					
					<View
						android:layout_width="1dp"
						android:layout_margin="5dp"
						android:background="@color/gray_btn_bg_color"
						android:layout_height="match_parent" />
				
				
				</LinearLayout>
				
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="Small, Hashback"
						android:text="@{user.vehicle_type+`, `+user.vehicle_subtype}"
						android:textSize="12sp"
						android:textColor="#888888"
						android:layout_height="wrap_content" />
					
					<View
						android:layout_width="1dp"
						android:layout_margin="5dp"
						android:background="@color/gray_btn_bg_color"
						android:layout_height="match_parent" />
				
				
				</LinearLayout>
				
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="Manual"
						android:text="@{user.transmission}"
						android:textSize="12sp"
						android:textColor="#888888"
						android:layout_height="wrap_content" />
					
					<View
						android:layout_width="1dp"
						android:layout_margin="5dp"
						android:background="@color/gray_btn_bg_color"
						android:layout_height="match_parent" />
				
				
				</LinearLayout>
				
				<LinearLayout
					android:layout_width="wrap_content"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="Petrol"
						android:text="@{user.fuel_type}"
						android:textSize="12sp"
						android:textColor="#888888"
						android:layout_height="wrap_content" />
				
				
				</LinearLayout>
			
			</LinearLayout>
			
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/gray_btn_bg_color" />
			
			<LinearLayout
				
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal">
				
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".5"
					android:orientation="vertical"
					android:paddingStart="5dp"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						tool:text="Pickup Address"
						android:text="@{user.isDifferentCity(user.is_different_city_drop) ? @string/pick_up_address :@string/pick_up_drop_address}"
						
						android:textSize="12sp"
						android:textColor="@color/black"
						android:layout_height="wrap_content" />
					
					<TextView
						android:layout_width="match_parent"
						tool:text="A-68,69 Sunder Singh Bhandari Nagar, Swej Farm, Sodala, Jaipur, Rajasthan 302019"
						android:text='@{user.pick_address}'
						android:textSize="12sp"
						android:ellipsize="end"
						android:maxLines="3"
						android:textColor="#888888"
						android:padding="@dimen/_5sdp"
						android:layout_height="wrap_content" />
				
				
				</LinearLayout>
				
				<View
					android:layout_width="1dp"
					android:visibility="@{user.isDifferentCity(user.is_different_city_drop) ?View.VISIBLE:View.GONE}"
					
					android:background="@color/gray_btn_bg_color"
					android:layout_height="match_parent" />
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".5"
					android:gravity="left"
					android:visibility="@{user.isDifferentCity(user.is_different_city_drop) ?View.VISIBLE:View.GONE}"
					
					android:paddingStart="10dp"
					android:orientation="vertical"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						android:text="@string/drop_address"
						android:textSize="12sp"
						android:textColor="@color/black"
						android:layout_height="wrap_content" />
					
					<TextView
						android:layout_width="wrap_content"
						android:textSize="12sp"
						tool:text="A-68,69 Sunder Singh Bhandari Nagar, Swej Farm, Sodala, Jaipur, Rajasthan 302019"
						android:text='@{user.drop_address}'
						android:textColor="#888888"
						android:padding="@dimen/_5sdp"
						android:ellipsize="end"
						android:maxLines="3"
						android:layout_height="wrap_content" />
				
				
				</LinearLayout>
			</LinearLayout>
			
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/gray_btn_bg_color" />
			
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content">
				
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".1"
					android:orientation="vertical"
					android:gravity="center_vertical"
					android:paddingStart="10dp"
					
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						android:text="@string/pickup_time"
						android:textSize="10sp"
						android:paddingTop="5dp"
						android:layout_height="wrap_content" />
					
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						
						android:gravity="center_vertical">
						
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							tool:text="22"
							android:text='@{user.getFormatedDAte(user.from_date_time,"DAY")}'
							android:textColor="@color/profile_top_back_color"
							android:textSize="25sp"
							android:textStyle="bold" />
						
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingStart="4dp"
							android:gravity="center_vertical">
							
							
							<TextView
								
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:textColor="@color/profile_top_back_color"
								tool:text="MAY"
								android:text='@{user.getFormatedDAte(user.from_date_time,"Month")}'
								android:textSize="10sp" />
							
							<TextView
								
								android:layout_width="wrap_content"
								
								android:layout_height="wrap_content"
								
								
								android:text='@{user.getFormatedDAte(user.from_date_time,"Time")}'
								
								tool:text="4:45 PM"
								android:textSize="10sp" />
						</LinearLayout>
					
					</LinearLayout>
				
				</LinearLayout>
				
				<ImageView
					android:layout_width="wrap_content"
					android:src="@drawable/date_arrow"
					android:layout_height="match_parent" />
				
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".1"
					android:paddingStart="5dp"
					android:orientation="vertical"
					android:gravity="center_vertical"
					android:layout_height="wrap_content">
					
					<TextView
						android:layout_width="wrap_content"
						android:text="@string/drop_off_time"
						android:textSize="10sp"
						android:paddingTop="5dp"
						android:layout_height="wrap_content" />
					
					<LinearLayout
						android:layout_width="match_parent"
						android:layout_height="wrap_content"
						
						android:gravity="center_vertical">
						
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							tool:text="22"
							android:text='@{user.getFormatedDAte(user.to_date_time,"DAY")}'
							android:textColor="@color/profile_top_back_color"
							android:textSize="25sp"
							android:textStyle="bold" />
						
						<LinearLayout
							android:orientation="vertical"
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:paddingStart="4dp"
							android:gravity="center_vertical">
							
							
							<TextView
								android:id="@+id/textView"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								tool:text="MAY"
								android:text='@{user.getFormatedDAte(user.to_date_time,"Month")}'
								android:textColor="@color/profile_top_back_color"
								android:textSize="10sp" />
							
							<TextView
								android:id="@+id/textView3"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:text='@{user.getFormatedDAte(user.to_date_time,"Time")}'
								tool:text="4:45 PM"
								android:textSize="10sp" />
						</LinearLayout>
					
					</LinearLayout>
				
				</LinearLayout>
				<LinearLayout
					android:layout_width="0dp"
					android:layout_weight=".15"
					app:manageExtended="@{user}"
					
					android:layout_height="wrap_content">
					<ImageView
						android:layout_width="wrap_content"
						android:layout_height="match_parent"
						android:src="@drawable/date_arrow" />
					
					<LinearLayout
						android:layout_width="0dp"
						android:layout_height="wrap_content"
						android:layout_weight=".1"
						android:gravity="center_vertical"
						android:orientation="vertical"
						android:paddingStart="5dp">
						
						<TextView
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:paddingTop="5dp"
							android:text="@string/extended_date"
							android:textAllCaps="true"
							android:textSize="10sp" />
						
						<LinearLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							
							android:gravity="center_vertical">
							
							<TextView
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								tool:text="22"
								android:text='@{user.getFormatedDAte(user.extended_date_time,"DAY")}'
								
								android:textColor="@color/profile_top_back_color"
								android:textSize="25sp"
								android:textStyle="bold" />
							
							<LinearLayout
								android:layout_width="match_parent"
								android:layout_height="wrap_content"
								android:gravity="center_vertical"
								android:orientation="vertical"
								android:paddingStart="4dp">
								
								
								<TextView
									
									android:layout_width="wrap_content"
									android:layout_height="match_parent"
									tool:text="MAY"
									android:text='@{user.getFormatedDAte(user.extended_date_time,"Month")}'
									
									android:textColor="@color/profile_top_back_color"
									android:textSize="10sp" />
								
								<TextView
									
									android:layout_width="wrap_content"
									
									android:layout_height="wrap_content"
									
									android:text='@{user.getFormatedDAte(user.extended_date_time,"Time")}'
									
									tool:text="4:45 PM"
									android:textSize="10sp" />
							</LinearLayout>
						
						</LinearLayout>
					
					</LinearLayout>
				</LinearLayout>
			</LinearLayout>
			
			<View
				android:layout_width="match_parent"
				android:background="@color/gray_btn_bg_color"
				android:layout_height="1dp" />
			
			<LinearLayout
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:orientation="horizontal"
				android:padding="@dimen/_5sdp"
				app:setTime="@{user}"
				android:gravity="center_horizontal"
				android:background="#F1F1F1">
				
				<TextView
					android:layout_width="wrap_content"
					android:text="@string/days"
					android:textColor="@color/profile_top_back_color"
					android:textSize="@dimen/_12sdp"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					tool:text=" 05"
					android:text="@{user.days}"
					android:textSize="@dimen/_12sdp"
					android:textStyle="bold"
					android:textColor="@color/profile_top_back_color"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					android:text="@string/hours"
					android:textSize="@dimen/_12sdp"
					android:textColor="@color/profile_top_back_color"
					android:layout_marginStart="@dimen/_5sdp"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					tool:text=" 05"
					android:text="@{user.hours}"
					
					android:textSize="@dimen/_12sdp"
					android:textStyle="bold"
					android:textColor="@color/profile_top_back_color"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					android:text="@string/minutes"
					android:textSize="@dimen/_12sdp"
					android:textColor="@color/profile_top_back_color"
					android:layout_marginStart="@dimen/_5sdp"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					tool:text=" 05"
					android:text="@{user.minutes}"
					
					android:textSize="@dimen/_12sdp"
					android:textStyle="bold"
					android:textColor="@color/profile_top_back_color"
					android:layout_height="wrap_content" />
				
				<TextView
					android:layout_width="wrap_content"
					android:text="@string/seconds"
					android:textSize="@dimen/_12sdp"
					android:textColor="@color/profile_top_back_color"
					android:layout_marginStart="@dimen/_5sdp"
					android:layout_height="wrap_content" />
				
				<TextView
					
					android:layout_width="wrap_content"
					tool:text="05"
					android:text="@{user.seconds}"
					
					android:textSize="@dimen/_12sdp"
					android:textStyle="bold"
					android:textColor="@color/profile_top_back_color"
					android:layout_height="wrap_content" />
				
				
			</LinearLayout>
			
			<View
				android:layout_width="match_parent"
				android:layout_height="1dp"
				android:background="@color/gray_btn_bg_color" />
		</LinearLayout>
	
</layout>